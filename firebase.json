{
  "hosting": {
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "trailingSlash":false,
    "cleanUrls":true,
    "public": "build/client",
    "frameworksBackend": {
      "region": "europe-west1"
    },
    "rewrites": [
      { "source": "**", "destination": "/index.html" }
    ]
  }
}
